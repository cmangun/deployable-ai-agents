{
  "name": "deployable-ai-agents",
  "version": "0.1.0",
  "description": "Deployable agent service with tool calling and policy controls",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "lint": "eslint src test --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "tools",
    "mcp"
  ],
  "author": "Christopher Mangun <cmangun@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
